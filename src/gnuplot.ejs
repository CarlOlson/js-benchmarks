<% runs.forEach(({points}, index) => { %>
$data<%= index+1 %> << END
# X Y<% points.forEach(([x, y]) => { %>
<%= x %> <%= y %><% }) %>
END
<% }) %>

# IBM palette, colorblind accessible
set style line 1 linecolor rgb '#dc267f' linewidth 1 pointtype 9 pointsize 1
set style line 2 linecolor rgb '#785ef0' linewidth 1 pointtype 7 pointsize 1
set style line 3 linecolor rgb '#ffb000' linewidth 1 pointtype 5 pointsize 1
set style line 4 linecolor rgb '#648fff' linewidth 1 pointtype 13 pointsize 1
set style line 5 linecolor rgb '#fe6100' linewidth 1 pointtype 11 pointsize 1

set terminal svg size 800,600 enhanced font 'Verdana,12'

set output '<%= output %>'
set xlabel '<%= xlabel %>'
set ylabel '<%= ylabel %>'

set key left top

set decimalsign locale ""
set decimalsign ","

set logscale x
set format x "%'.0f"
set xtics (<%= xtics.join(", ") %>)

set title '<%= title %>'

plot \<% runs.forEach(({name}, index) => { %>
  $data<%= index+1 %> title '<%= name %>' with linespoints linestyle <%= index+1 %><%= index < (runs.length - 1) ? ", \\" : "" %><% }) %>